{
  "name": "react-node-mongoose",
  "version": "1.0.0",
  "description": "web development  with react, node and mongoose",
  "main": "server.js",
  "scripts": {
    "eslintCheck": "eslint --ignore-path .gitignore .",
    "server": "nodemon -r dotenv/config server.js",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
